package test;
import java.io.File;
import java.util.*;
/*
     palindrome string r those string which a same from both side   from forward and backward 
*/
public  class demo {
	
	
	public static void main(String[] args)   throws Exception
	{ 
		String str = "shahalam";
		String str1 = "";
		
		for(int i=str.length()-1;i>=0;i--)
		{
			str1=str1 + str.charAt(i);
		}
		System.out.print(str1);
}
}


