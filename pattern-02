package test;

import java.util.*;
/*
        *000*000*
		0*00*00*0
		00*0*0*00
		000***000
*/
public  class demo {
	
	
	public static void main(String[] args)   throws Exception
	{ 
		 for(int i=1;i<=10;i++)
		 {
			 for(int j=1;j<=i;j++)
			 {
				 System.out.print(i*j+" ");
			 }
			 System.out.println();
		 }
}
}


